# API Configuration
API_BASE_URL=https://api.staging.com
API_TIMEOUT=30000

# App Configuration
APP_NAME=RNTemplate
APP_VERSION=1.0.0
BUILD_NUMBER=1

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PUSH_NOTIFICATIONS=true

# Environment
NODE_ENV=staging
LOG_LEVEL=warn

# Third Party Services
GOOGLE_MAPS_API_KEY=your_staging_google_maps_api_key
STRIPE_PUBLISHABLE_KEY=your_staging_stripe_key
